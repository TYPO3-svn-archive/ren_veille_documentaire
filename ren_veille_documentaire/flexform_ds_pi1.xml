<T3DataStructure>
	<meta>
       <langDisable>1</langDisable>
	</meta>
	<sheets>
		<configuration>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.configuration</sheetTitle>
				</TCEforms>
				<el>
					<detailsveille>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.detailsveille</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</detailsveille>
					<detailsnotice>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.detailsnotice</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</detailsnotice>
					<maxnotice>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.maxnotice</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<default></default>
							</config>
						</TCEforms>
					</maxnotice>
					<nbnoticepage>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.nbnoticepage</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<default></default>
							</config>
						</TCEforms>
					</nbnoticepage>
					<pagenews>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.pagenews</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
							</config>
						</TCEforms>
					</pagenews>
					<template>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.template</label>
							<config>
								<type>input</type>
								<size>60</size>
								<default></default>
							</config>
						</TCEforms>
					</template>
					<dateFormat>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.dateFormat</label>
							<config>
								<type>input</type>
								<size>60</size>
								<default>%e/%m/%Y</default>
							</config>
						</TCEforms>
					</dateFormat>
					<resumecrop>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.resumecrop</label>
							<config>
								<type>input</type>
								<size>60</size>
								<default></default>
							</config>
						</TCEforms>
					</resumecrop>
					<sorting>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.sorting</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.sorting.0</numIndex>
										<numIndex index="1">TITRE</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.sorting.1</numIndex>
										<numIndex index="1">DATE</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.sorting.2</numIndex>
										<numIndex index="1">SORTING</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</sorting>
