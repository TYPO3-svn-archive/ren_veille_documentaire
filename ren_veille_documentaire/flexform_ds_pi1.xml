<T3DataStructure>
	<meta>
       <langDisable>1</langDisable>
	</meta>
	<sheets>
		<configuration>
			<ROOT>
				<TCEforms>
					<sheetTitle>Configuration</sheetTitle>
				</TCEforms>
				<el>
					<pagedetail>
						<TCEforms>
							<label>Page de détail</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</pagedetail>
					<maxnotice>
						<TCEforms>
							<label>Nombre de notices maximum à afficher</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<default></default>
							</config>
						</TCEforms>
					</maxnotice>
					<nbnoticepage>
						<TCEforms>
							<label>Nombre de notices à afficher par page</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<default></default>
							</config>
						</TCEforms>
					</nbnoticepage>
					<pagenews>
						<TCEforms>
							<label>Page d'affichage des actualités liées</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
							</config>
						</TCEforms>
					</pagenews>
					<template>
						<TCEforms>
							<label>template</label>
							<config>
								<type>input</type>
								<size>60</size>
								<default></default>
							</config>
						</TCEforms>
					</template>
					<dateFormat>
						<TCEforms>
							<label>Format de date</label>
							<config>
								<type>input</type>
								<size>60</size>
								<default>%e/%m/%Y</default>
							</config>
						</TCEforms>
					</dateFormat>
				</el>
			</ROOT>
		</configuration>
		<selection>
			<ROOT>
				<TCEforms>
					<sheetTitle>Sélection</sheetTitle>
				</TCEforms>
				<el>
					<veilles>
						<TCEforms>
							<label>Veilles documentaire</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_renveilledocumentaire_veilles</allowed>
								<size>5</size>
								<maxitems>50</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</veilles>
					<operateur>
						<TCEforms>
							<label>opérateur</label>
							<config>
								<type>select</type>
									<items type="array">
										<numIndex index="0" type="array">
											<numIndex index="0">OU</numIndex>
											<numIndex index="1">0</numIndex>
										</numIndex>
										<numIndex index="1" type="array">
											<numIndex index="0">ET</numIndex>
											<numIndex index="1">1</numIndex>
										</numIndex>
									</items>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</operateur>
				</el>
			</ROOT>
		</selection>
	</sheets>
</T3DataStructure>