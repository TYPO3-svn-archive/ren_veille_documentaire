			</ROOT>
		</configuration>
		<selection>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.selection</sheetTitle>
				</TCEforms>
				<el>
					<veilles>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.veilles</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_renveilledocumentaire_veilles</allowed>
								<size>5</size>
								<maxitems>50</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</veilles>
					<operateur>
						<TCEforms>
							<label>LLL:EXT:ren_veille_documentaire/locallang_db.xml:flexform.pi1.operateur</label>
							<config>
								<type>select</type>
									<items type="array">
										<numIndex index="0" type="array">
											<numIndex index="0">OU</numIndex>
											<numIndex index="1">0</numIndex>
										</numIndex>
										<numIndex index="1" type="array">
											<numIndex index="0">ET</numIndex>
											<numIndex index="1">1</numIndex>
										</numIndex>
									</items>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</operateur>
				</el>
			</ROOT>
		</selection>
	</sheets>
</T3DataStructure>